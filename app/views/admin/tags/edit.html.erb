<div class="container">
  <div class="row">
    <div class="col">
      <h3 class="my-3">タグ編集</h3>
      <%= form_with model:@tag,url: admin_tag_path(@tag.id), local:true do |f| %>
        <%= f.label :タグ名 %>
        <%= f.text_field :tag_name %>
        <%= f.submit "変更を保存", class: 'btn btn-success' %>
      <% end %>
    </div>
  </div>
</div>